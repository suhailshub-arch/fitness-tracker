{
  "name": "workout-service",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "npm run db:seed && tsx --watch src/index.ts",
    "build": "npm run prisma:generate && tsc",
    "start": "NODE_ENV=production node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "test:db": "docker-compose -f docker-compose.test.yml up -d",
    "test:db:down": "docker-compose -f docker-compose.test.yml down",
    "test:prepare": "dotenv -e .env.test -- cross-env NODE_ENV=test npx prisma db push",
    "test:unit": "jest",
    "test:integration": "npm run test:db && npm run test:prepare && jest && npm run test:db:down"
  },
  "prisma": {
    "seed": "npm run db:seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.8.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.24",
    "@types/supertest": "^6.0.3",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
